<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.champion.map_assignment_alarm">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <service
            android:name=".RingtonePlayingService"
            android:exported="false" />

        <receiver android:name=".Alarm_BroadCast_Receiver"/>
        <receiver android:name=".MainActivity$MyBroadCastReceiver"/>
        <activity android:name=".Date_Picker_practice">

        </activity>
        <activity android:name=".StepCounterHandle"></activity>
    </application>


</manifest>